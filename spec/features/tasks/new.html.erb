<h1>Create a New Task</h1>
<%= form_for [:list, @task] do |f| %>
  <p><%= f.text_field :title, placeholder: "Title" %><br/>
  <p><%= f.text_area :body, placeholder: "Body" %><br/>
    <p><%= f.label :start_date %>
    <%= f.date_field :start_date %><br/>
  <p><%= f.label :due_date %>
  <%= f.date_field :due_date %><br/>
  <p><%= f.hidden_field :list_id, value: params[:list_id]%>
  <p><%= f.submit "Create Task" %></p>
<% end %>
