<!DOCTYPE html>
<html>
<head>
  <title>List.ly</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
  <body>
    <%= render "layouts/navbar"%>

    <% flash.each do |type, message| %>
      <div class="alert flash <%= type %>">
        <%= message %>
      </div>
    <% end %>

    <h1>Welcome to List.ly</h1>

    <%= yield %>

  </body>
</html>
